# Papercups Integration
# Set to "true" to enable Papercups chat widget
PAPERCUPS_ENABLED=false

# Your Papercups account ID (required when enabled)
PAPERCUPS_ACCOUNT_ID=

# Webhook verification token (optional)
PAPERCUPS_TOKEN=

# Specific inbox ID to use (optional)
PAPERCUPS_INBOX_ID=

# Custom Papercups host (optional, defaults to https://app.papercups.io)
PAPERCUPS_HOST=

